<!-- C160 Transall BARANGER Emmanuel (2009/2010)
           Updated BARANGER Emmanuel (01/2020)
           Updated BARANGER Emmanuel (04/2020)
           Updated BARANGER Emmanuel (03/2025)

     sources         : http://www.aviation-fr.info/guide/transall.php
                       http://faq-fra.aviatechno.net/guide/transall.php
                       https://en.wikipedia.org/wiki/Transall_C-160

     Wingspan           : 40.00 m    (    131 ft 3 in )
     Lenght             : 32.40 m    (    106 ft 3 in )
     Height             : 11.65 m    (     38 ft 2 in )
     Empty mass         : 28760 kg   (  63404 lb      )
     Max takeoff weight : 51000 kg   ( 112436 lb      )
     Maximum speed      :   513 km/h (    277 kn      ) at 4875 m ( 15994 ft )
     Stall speed        :   177 km/h (     96 kn      ) flaps down
-->
<airplane mass="63404" version="YASIM_VERSION_CURRENT" mtow-lbs="112436">

    <approach speed="96" aoa="4" fuel="0.2">
      <control-setting axis="/controls/engines/engine[0]/throttle"        value="0.50"/>
      <control-setting axis="/controls/engines/engine[0]/mixture"         value="1.00"/>
      <control-setting axis="/controls/engines/engine[0]/propeller-pitch" value="0.77"/>

      <control-setting axis="/controls/engines/engine[1]/throttle"        value="0.50"/>
      <control-setting axis="/controls/engines/engine[1]/mixture"         value="1.00"/>
      <control-setting axis="/controls/engines/engine[1]/propeller-pitch" value="0.77"/>

      <control-setting axis="/controls/flight/flaps"                      value="1"/>
      <control-setting axis="/controls/gear/gear-down"                    value="1"/>
    </approach>

    <cruise speed="260" alt="15994" fuel="0.5">
      <control-setting axis="/controls/engines/engine[0]/throttle"        value="1.0"/>
      <control-setting axis="/controls/engines/engine[0]/mixture"         value="0.3"/>
      <control-setting axis="/controls/engines/engine[0]/propeller-pitch" value="0.9"/>

      <control-setting axis="/controls/engines/engine[1]/throttle"        value="1.0"/>
      <control-setting axis="/controls/engines/engine[1]/mixture"         value="0.3"/>
      <control-setting axis="/controls/engines/engine[1]/propeller-pitch" value="0.9"/>

      <control-setting axis="/controls/flight/flaps"                      value="0"/>
      <control-setting axis="/controls/gear/gear-down"                    value="0"/>
    </cruise>

    <cockpit x="13.159" y="0.639" z="-2.161"/>

    <!-- Fuselage -->
    <fuselage ax=" 16.160" ay=" 0.000" az="-3.677" bx=" 13.300" by=" 0.000" bz="-3.145" width="3.400" taper="0.20" midpoint="1"/>
    <fuselage ax=" 13.300" ay=" 0.000" az="-3.145" bx=" 11.895" by=" 0.000" bz="-3.038" width="3.800" taper="0.90" midpoint="1"/>
    <fuselage ax=" 11.895" ay=" 0.000" az="-3.038" bx="  9.539" by=" 0.000" bz="-2.938" width="4.200" taper="0.90" midpoint="1"/>
    <fuselage ax="  9.539" ay=" 0.000" az="-2.938" bx=" -1.847" by=" 0.000" bz="-2.753" width="4.200" taper="1.00" midpoint="0"/>
    <fuselage ax=" -1.847" ay=" 0.000" az="-2.753" bx="-11.994" by=" 0.000" bz="-0.777" width="4.200" taper="0.30" midpoint="0"/>
    <fuselage ax="-11.994" ay=" 0.000" az="-0.777" bx="-16.174" by=" 0.000" bz="-0.600" width="1.200" taper="0.40" midpoint="0"/>

    <fuselage ax="  6.923" ay=" 2.417" az="-4.328" bx="  6.149" by=" 2.417" bz="-4.328" width="1.400" taper="0.20" midpoint="1"/>
    <fuselage ax="  6.149" ay=" 2.417" az="-4.328" bx="  0.597" by=" 2.417" bz="-4.328" width="1.400" taper="1.00" midpoint="1"/>
    <fuselage ax="  0.597" ay=" 2.417" az="-4.328" bx=" -1.106" by=" 2.417" bz="-4.328" width="1.400" taper="0.20" midpoint="0"/>

    <fuselage ax="  6.923" ay="-2.417" az="-4.328" bx="  6.149" by="-2.417" bz="-4.328" width="1.400" taper="0.20" midpoint="1"/>
    <fuselage ax="  6.149" ay="-2.417" az="-4.328" bx="  0.597" by="-2.417" bz="-4.328" width="1.400" taper="1.00" midpoint="1"/>
    <fuselage ax="  0.597" ay="-2.417" az="-4.328" bx=" -1.106" by="-2.417" bz="-4.328" width="1.400" taper="0.20" midpoint="0"/>

    <!-- Engines -->
    <fuselage ax="  7.412" ay=" 5.411" az="-1.559" bx="  6.149" by=" 5.411" bz="-1.559" width="1.400" taper="0.70" midpoint="1"/>
    <fuselage ax="  6.149" ay=" 5.411" az="-1.559" bx="  1.099" by=" 5.411" bz="-1.559" width="1.400" taper="0.70" midpoint="0"/>

    <fuselage ax="  7.412" ay="-5.411" az="-1.559" bx="  6.149" by="-5.411" bz="-1.559" width="1.400" taper="0.70" midpoint="1"/>
    <fuselage ax="  6.149" ay="-5.411" az="-1.559" bx="  1.099" by="-5.411" bz="-1.559" width="1.400" taper="0.70" midpoint="0"/>

    <!-- Wing -->
    <wing x="2.451" y="0" z="-1.060"
           chord="4.915"
           length="6.550"
           taper="1.0"
           camber="0.004">
           <stall   aoa="20" width="1" peak="2.5"/>
           <flap0 start="0.3"  end="1" lift="1.5" drag="1.8"/>

           <control-input  control="FLAP0" axis="/controls/flight/flaps"/>

           <control-output control="FLAP0" prop="/surface-positions/flap-pos-norm"/>

           <control-speed  control="FLAP0" transition-time="5"/>
    </wing>

    <wing x="2.451" y="6.544" z="-1.060"
          append="1"
          chord="4.915"
          length="13.530"
          taper="0.50"
          dihedral="4.5">
          <stall     aoa="20"  width="1"     peak="2.5"/>
          <flap0   start="0"     end="0.621" lift="1.5" drag="1.8"/>
          <flap1   start="0.621" end="1.000" lift="1.2" drag="1.2"/>
          <spoiler start="0.17"  end="0.58"  lift="6.0" drag="4.0"/>

          <control-input  control="SPOILER" axis="/controls/flight/spoilers"/>
          <control-input  control="FLAP0"   axis="/controls/flight/flaps"/>
          <control-input  control="FLAP1"   axis="/controls/flight/aileron"                 split="true"/>
          <control-input  control="FLAP1"   axis="/controls/flight/aileron-trim"            split="true"/>

          <control-output control="SPOILER" prop="/surface-positions/speedbrake-pos-norm"/>
          <control-output control="FLAP0"   prop="/surface-positions/flap-pos-norm"/>
          <control-output control="FLAP1"   prop="/surface-positions/left-aileron-pos-norm"  side="left"/>
          <control-output control="FLAP1"   prop="/surface-positions/right-aileron-pos-norm" side="right"/>

          <control-speed  control="FLAP0"   transition-time="5"/>
    </wing>

    <!-- Elevator -->
    <hstab x="-13.742" y="0" z="-0.803"
           chord="4.107"
           length="7.367"
           taper="0.46"
           sweep="3">
           <stall   aoa="18" width="1" peak="1.5"/>
           <flap0 start="0.15" end="1" lift="1.4" drag="1.2"/>

           <control-input  control="FLAP0" axis="/controls/flight/elevator"/>
           <control-input  control="FLAP0" axis="/controls/flight/elevator-trim"/>

           <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>
    </hstab>

    <!-- Ruder -->
    <vstab x="-13.002" y="0" z="-0.148"
           chord="6.320"
           length="6.158"
           taper="0.55"
           sweep="14">
           <stall   aoa="16" width="1" peak="1.5"/>
           <flap0 start="0"    end="1" lift="1.4" drag="1.2"/>

           <control-input  control="FLAP0" axis="/controls/flight/rudder"      invert="true"/>
           <control-input  control="FLAP0" axis="/controls/flight/rudder-trim" invert="true"/>

           <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"/>
    </vstab>

    <!-- Informations : http://en.wikipedia.org/wiki/Rolls-Royce_Tyne
                        http://www.aviation-fr.info/guide/transall.php

         Engines : Wright Rolls-Royce Tyne 22
         type    : Twin-spool turboprop
         Power   : 6100 ch
         Masse   : 2489 lb (1129 kg)
         RPM     : 2400 - 2600
    -->
    <!-- Engine left -->
    <propeller x="6.033" y="5.411" z="-1.424"
               mass="2489"
               moment="82"
               radius="2.8"
               cruise-speed="260"
               cruise-rpm="2500"
               cruise-power="5800"
               cruise-alt="15994"
               min-rpm="150"
               max-rpm="2600">
               <actionpt x="7.750" y="5.411" z="-1.424"/>
               <control-input control="ADVANCE" axis="/controls/engines/engine[0]/propeller-pitch"/>
               <turbine-engine eng-power="6100"
                               eng-rpm="2600"
                               alt="8000"
                               flat-rating="4150"
                               min-n2="70"
                               max-n2="100">
                               <control-input control="THROTTLE"    axis="/controls/engines/engine[0]/throttle"/>
                               <control-input control="STARTER"     axis="/controls/engines/engine[0]/starter"/>
                               <control-input control="MAGNETOS"    axis="/controls/engines/engine[0]/magnetos"/>
                               <control-input control="MIXTURE"     axis="/controls/engines/engine[0]/mixture"/>
                               <control-input control="PROPFEATHER" axis="/controls/engines/engine[0]/propeller-feather"/>
               </turbine-engine>
    </propeller>

    <!-- Engine right -->
    <propeller x="6.033" y="-5.411" z="-1.424"
               mass="2489"
               moment="82"
               radius="2.8"
               cruise-speed="260"
               cruise-rpm="2500"
               cruise-power="5800"
               cruise-alt="15994"
               min-rpm="150"
               max-rpm="2600">
               <actionpt x="7.750" y="-5.411" z="-1.424"/>
               <control-input control="ADVANCE" axis="/controls/engines/engine[1]/propeller-pitch"/>
               <turbine-engine eng-power="6100"
                               eng-rpm="2600"
                               alt="8000"
                               flat-rating="4150"
                               min-n2="70"
                               max-n2="100">
                               <control-input control="THROTTLE"    axis="/controls/engines/engine[1]/throttle"/>
                               <control-input control="STARTER"     axis="/controls/engines/engine[1]/starter"/>
                               <control-input control="MAGNETOS"    axis="/controls/engines/engine[1]/magnetos"/>
                               <control-input control="MIXTURE"     axis="/controls/engines/engine[1]/mixture"/>
                               <control-input control="PROPFEATHER" axis="/controls/engines/engine[1]/propeller-feather"/>
               </turbine-engine>
    </propeller>

    <!-- Noze wheel -->
    <gear x="12.970" y="0.000" z="-5.737"
          compression="0.1"
          spring="6"
          damp="1.50"
          stiction="1">
          <control-input  control="STEER"  axis="/controls/flight/rudder" square="true" src0="-1.0" src1="1.0" dst0="-0.8" dst1="0.8"/>
          <control-input  control="EXTEND" axis="/controls/gear/gear-down"/>

          <control-output control="EXTEND" prop="/gear/gear[0]/position-norm"/>

          <control-speed  control="EXTEND" transition-time="5"/>
    </gear>

    <!-- Left wheel 1 -->
    <gear x="3.266" y="2.502" z="-5.792"
          compression="0.15"
          spring="6"
          damp="1.5"
          sfric="1.2"
          dfric="1.2"
          stiction="1">
          <control-input  control="BRAKE"  axis="/controls/gear/brake-left"/>
          <control-input  control="BRAKE"  axis="/controls/gear/brake-parking"/>
          <control-input  control="EXTEND" axis="/controls/gear/gear-down"/>

          <control-output control="EXTEND" prop="/gear/gear[1]/position-norm"/>

          <control-speed  control="EXTEND" transition-time="5.1"/>
    </gear>

    <!-- Left wheel 2 -->
    <gear x="1.702" y="2.502" z="-5.802"
          compression="0.15"
          spring="6"
          damp="1.5"
          sfric="1.2"
          dfric="1.2"
          stiction="1">
          <control-input  control="BRAKE"  axis="/controls/gear/brake-left"/>
          <control-input  control="BRAKE"  axis="/controls/gear/brake-parking"/>
          <control-input  control="EXTEND" axis="/controls/gear/gear-down"/>

          <control-output control="EXTEND" prop="/gear/gear[2]/position-norm"/>

          <control-speed  control="EXTEND" transition-time="5.1"/>
    </gear>

    <!-- Right wheel 1 -->
    <gear x="3.266" y="-2.502" z="-5.792"
          compression="0.15"
          spring="6"
          damp="1.5"
          sfric="1.2"
          dfric="1.2"
          stiction="1">
          <control-input  control="BRAKE"  axis="/controls/gear/brake-right"/>
          <control-input  control="BRAKE"  axis="/controls/gear/brake-parking"/>
          <control-input  control="EXTEND" axis="/controls/gear/gear-down"/>

          <control-output control="EXTEND" prop="/gear/gear[3]/position-norm"/>

          <control-speed  control="EXTEND" transition-time="5.2"/>
    </gear>

    <!-- Right wheel 2 -->
    <gear x="1.702" y="-2.502" z="-5.802"
          compression="0.15"
          spring="6"
          damp="1.5"
          sfric="1.2"
          dfric="1.2"
          stiction="1">
          <control-input  control="BRAKE"  axis="/controls/gear/brake-right"/>
          <control-input  control="BRAKE"  axis="/controls/gear/brake-parking"/>
          <control-input  control="EXTEND" axis="/controls/gear/gear-down"/>

          <control-output control="EXTEND" prop="/gear/gear[4]/position-norm"/>

          <control-speed  control="EXTEND" transition-time="5.2"/>
    </gear>

    <!-- 5435 litres = 11982 lb -->
    <tank x="2.7" y=" 6" z="-1.0" capacity="11982"/>
    <!-- 5425 litres = 11960 lb -->
    <tank x="2.7" y=" 0" z="-4.7" capacity="11960"/>
    <!-- 5435 litres = 11982 lb -->
    <tank x="2.7" y="-6" z="-1.0" capacity="11982"/>

    <!-- Displacement mass -->
    <ballast x="16" y="0" z="-3.7" mass="9420"/>

</airplane>

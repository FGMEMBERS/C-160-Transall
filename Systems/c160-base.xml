<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <sim               include="views.xml">

    <fuel-fraction>1</fuel-fraction>

    <startup>
      <splash-texture>Aircraft/C-160-Transall/c160-splash.png</splash-texture>
    </startup>

    <sound>
      <path>Aircraft/C-160-Transall/Sounds/c160-sound.xml</path>
    </sound>

    <model>
      <path>Aircraft/C-160-Transall/Models/c160.xml</path>
      <shader-effect type="bool">false</shader-effect>
      <livery>
        <file        type="string"/>
        <specular>
          <red       type="float"> 0.03 </red>
          <green     type="float"> 0.03 </green>
          <blue      type="float"> 0.03 </blue>
        </specular>
      </livery>
    </model>

    <systems         include="rules.xml"/>

    <instrumentation include="initialisation.xml"/>

    <submodels>
      <serviceable   type="bool">true</serviceable>
      <path>Aircraft/C-160-Transall/Models/Effects/Parachuter/submodels.xml</path>
    </submodels>

    <multiplay>
      <chat_display>1</chat_display>
      <generic>
        <!-- -->
        <int    n="0"  type="int"/>
        <int    n="1"  type="int"/>
        <int    n="2"  type="int"/>
        <int    n="3"  type="int"/>
        <int    n="4"  type="int"/>
        <int    n="5"  type="int"/>
        <!-- -->
        <bool   n="0"  alias="/controls/jump-signal"/>
        <bool   n="1"  alias="/sim/crashed"/>
        <bool   n="2"  alias="/sim/rendering/hdr/hdr-enabled"/>
        <bool   n="3"  alias="/gear/gear[0]/wow"/>
        <bool   n="4"  alias="/gear/gear[1]/wow"/>
        <bool   n="5"  alias="/gear/gear[2]/wow"/>
        <bool   n="6"  alias="/gear/gear[3]/wow"/>
        <bool   n="7"  alias="/gear/gear[4]/wow"/>
        <!-- -->
        <bool   n="8"  alias="/controls/engines/engine[0]/cutoff"/>
        <bool   n="9"  alias="/controls/engines/engine[1]/cutoff"/>
        <!-- Lights -->
        <bool   n="11" alias="/controls/lighting/beacon"/>
        <bool   n="12" alias="/sim/model/lights/beacon/state"/>
        <bool   n="13" alias="/controls/lighting/strobe"/>
        <bool   n="14" alias="/sim/model/lights/sbc1/state"/>
        <bool   n="15" alias="/controls/lighting/nav-lights"/>
        <bool   n="16" alias="/controls/lighting/landing-lights"/>
        <!-- -->
        <bool   n="17" alias="/controls/engines/engine[0]/reverser"/>
        <bool   n="18" alias="/controls/engines/engine[1]/reverser"/>
        <!-- Doors -->
        <float  n="0"  alias="/sim/model/door-positions/radar/position-norm"/>
        <float  n="1"  alias="/sim/model/door-positions/passenger/position-norm"/>
        <!-- -->
        <float  n="2"  alias="/position/gear-agl-m">0</float>
        <float  n="3"  alias="/position/altitude-ft">0</float>
        <!-- -->
        <float  n="4"  alias="/engines/engine[0]/n1"/>
        <float  n="5"  alias="/engines/engine[1]/n1"/>
        <!-- Doors -->
        <float  n="6"  alias="/controls/engines/engine[0]/propeller-pitch"/>
        <float  n="7"  alias="/controls/engines/engine[0]/propeller-pitch"/>
        <!-- -->
        <float  n="8"  alias="/gear/gear[0]/rollspeed-ms"/>
        <float  n="9"  alias="/gear/gear[1]/rollspeed-ms"/>
        <float  n="10" alias="/gear/gear[2]/rollspeed-ms"/>
        <float  n="11" alias="/gear/gear[3]/rollspeed-ms"/>
        <float  n="12" alias="/gear/gear[4]/rollspeed-ms"/>
        <!-- -->
        <float  n="13" alias="/gear/gear[0]/compression-norm"/>
        <float  n="14" alias="/gear/gear[1]/compression-norm"/>
        <float  n="15" alias="/gear/gear[2]/compression-norm"/>
        <float  n="16" alias="/gear/gear[3]/compression-norm"/>
        <float  n="17" alias="/gear/gear[4]/compression-norm"/>
        <!-- -->
        <float  n="18" alias="/gear/gear[0]/caster-angle-deg"/>
        <!-- Mode -->
        <string n="0"  alias="/sim/model/mode"/>
        <!-- For Bombable -->
        <int    n="10" alias="/controls/armament/trigger"/>
        <string n="9"  type="string"/>
      </generic>
      <refuel>
        <type>probe</type>
      </refuel>
    </multiplay>

    <flaps           include="flaps.xml"/>

    <help            include="help.xml"/>

    <menubar         include="../Dialogs/c160-menu.xml"/>

  </sim>

  <input             include="c160-keyboard.xml"/>

  <controls          include="controls.xml"/>

  <consumables       include="consumables.xml"/>

  <engines           include="engines.xml"/>

  <systems           include="systems.xml"/>

  <instrumentation   include="instrumentation.xml">
    <gsdi>
      <serviceable   type="bool">true</serviceable>
      <enabled       type="bool">true</enabled>
    </gsdi>
  </instrumentation>

  <nasal>
    <doors>
      <file>Aircraft/C-160-Transall/Nasal/doors.nas</file>
    </doors>
    <c160>
      <file>Aircraft/C-160-Transall/Nasal/hdr.nas</file>
      <file>Aircraft/C-160-Transall/Nasal/liveries.nas</file>
      <file>Aircraft/C-160-Transall/Nasal/light.nas</file>
      <file>Aircraft/C-160-Transall/Nasal/convert.nas</file>
      <file>Aircraft/C-160-Transall/Nasal/Electrical.nas</file>
      <file>Aircraft/C-160-Transall/Nasal/map.nas</file>
    </c160>
    <para>
      <file>Aircraft/C-160-Transall/Nasal/paratroopers.nas</file>
    </para>
    <engines>
      <file>Aircraft/Generic/Engines/generic-yasim-engine.nas</file>
      <file>Aircraft/C-160-Transall/Nasal/engines-yasim.nas</file>
    </engines>
  </nasal>

</PropertyList>

<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <lighting>
    <instruments-norm  type="float">0.8</instruments-norm>
    <landing-lights    type="bool">false</landing-lights>
    <beacon            type="bool">false</beacon>
    <strobe            type="bool">false</strobe>
    <nav-lights        type="bool">false</nav-lights>
    <cabin-lights      type="bool">false</cabin-lights>
    <wing-lights       type="bool">false</wing-lights>
    <taxi-light        type="bool">false</taxi-light>
  </lighting>

  <electric>
    <battery-switch    type="bool">false</battery-switch>
    <avionics-switch   type="bool">false</avionics-switch>
    <generator         type="int">0</generator>
    <engines>
      <engine          n="0">
        <fuel-pump     type="bool">false</fuel-pump>
      </engine>
      <engine          n="1">
        <fuel-pump     type="bool">false</fuel-pump>
      </engine>
    </engines>
  </electric>

  <engines>
    <engine            n="0">
      <on              type="bool">false</on>
      <rpm             type="float">0</rpm>
      <boost           type="double">0</boost>
      <cutoff          type="bool">true</cutoff>
      <magnetos        type="int">0</magnetos>
      <condition       type="double">0</condition>
      <condition-lever type="float">0</condition-lever>
    </engine>
    <engine            n="1">
      <on              type="bool">false</on>
      <rpm             type="float">0</rpm>
      <boost           type="double">0</boost>
      <cutoff          type="bool">true</cutoff>
      <magnetos        type="int">0</magnetos>
      <condition       type="double">0</condition>
      <condition-lever type="float">0</condition-lever>
    </engine>
  </engines>

  <gear>
    <brake-parking     type="bool">true</brake-parking>
    <tailwheel-lock    type="bool">false</tailwheel-lock>
  </gear>

  <flight>
    <rudder-trim       type="double">  0.00 </rudder-trim>
    <aileron-trim      type="double">  0.01 </aileron-trim>
    <elevator-trim     type="double"> -0.07 </elevator-trim>
  </flight>

</PropertyList>

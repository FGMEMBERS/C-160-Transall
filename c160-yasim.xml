<!-- C160 Transall BARANGER Emmanuel (2009/2010)

     sources         : http://www.aviation-fr.info/guide/transall.php

     Wingspan        : 40.00 m    (  131 ft 3 in)
     Lenght          : 32.40 m    (  106 ft 3 in)
     Height          : 11.65 m    (   38 ft 2 in)
     Empty mass      : 28760 kg   (63404 lb)
     Max speed       :   513 km/h (  277 kts)
-->

<airplane mass="63404">

    <approach speed="90" aoa="8" fuel="0.2">
      <control-setting axis="/controls/engines/engine[0]/throttle" value="0.5"/>
      <control-setting axis="/controls/engines/engine[0]/mixture" value="1.0"/>
      <control-setting axis="/controls/engines/engine[0]/propeller-pitch" value="0.77"/>
      <control-setting axis="/controls/engines/engine[1]/throttle" value="0.5"/>
      <control-setting axis="/controls/engines/engine[1]/mixture" value="1.0"/>
      <control-setting axis="/controls/engines/engine[1]/propeller-pitch" value="0.77"/>
      <control-setting axis="/controls/flight/flaps" value="1"/>
      <control-setting axis="/controls/gear/gear-down" value="1"/>
    </approach>

    <cruise speed="260" alt="20000" fuel="0.5">
      <control-setting axis="/controls/engines/engine[0]/throttle" value="1.0"/>
      <control-setting axis="/controls/engines/engine[0]/mixture" value="0.3"/>
      <control-setting axis="/controls/engines/engine[0]/propeller-pitch" value="0.9"/>
      <control-setting axis="/controls/engines/engine[1]/throttle" value="1.0"/>
      <control-setting axis="/controls/engines/engine[1]/mixture" value="0.3"/>
      <control-setting axis="/controls/engines/engine[1]/propeller-pitch" value="0.9"/>
      <control-setting axis="/controls/flight/flaps" value="0"/>
      <control-setting axis="/controls/gear/gear-down" value="0"/>
    </cruise>

    <cockpit x="13.159" y="0.639" z="-2.161"/>

    <!-- Fuselage -->
    <fuselage ax=" 16.160" ay="0" az="-3.677" bx=" 13.300" by="0" bz="-3.145" width="3.4" taper="0.2" midpoint="1"/>
    <fuselage ax=" 13.300" ay="0" az="-3.145" bx=" 11.895" by="0" bz="-3.038" width="3.8" taper="0.9" midpoint="1"/>
    <fuselage ax=" 11.895" ay="0" az="-3.038" bx="  9.539" by="0" bz="-2.938" width="4.2" taper="0.9" midpoint="1"/>
    <fuselage ax="  9.539" ay="0" az="-2.938" bx=" -1.847" by="0" bz="-2.753" width="4.2" taper="1.0" midpoint="0"/>
    <fuselage ax=" -1.847" ay="0" az="-2.753" bx="-11.994" by="0" bz="-0.777" width="4.2" taper="0.3" midpoint="0"/>
    <fuselage ax="-11.994" ay="0" az="-0.777" bx="-16.174" by="0" bz="-0.600" width="1.2" taper="0.4" midpoint="0"/>

    <!-- Engines -->
    <fuselage ax="7.412"  ay="5.411" az="-1.559" bx="1.099" by="5.411" bz="-1.559" width="1.4" taper="0.7" midpoint="0.2"/>

    <fuselage ax="7.412"  ay="-5.411" az="-1.559" bx="1.099" by="-5.411" bz="-1.559" width="1.4" taper="0.7" midpoint="0.2"/>

    <!-- Wing -->
    <mstab x="2.451" y="1.031" z="-1.060"
           chord="4.915"
           length="5.519"
           taper="1.0"
           camber="0.05">
           <stall aoa="9" width="1" peak="2.5"/>
           <flap0 start="0.166" end="1" lift="1.6" drag="1.6"/>

           <control-input axis="/controls/flight/flaps" control="FLAP0" invert="true"/>

           <control-output control="FLAP0" prop="/surface-positions/flap-pos-norm"/>

           <control-speed control="FLAP0" transition-time="5"/>
    </mstab>

    <wing x="2.451" y="6.544" z="-1.060"
          chord="4.915"
          length="13.530"
          taper="0.50"
	  dihedral="4.5"
          camber="0.05">
          <stall aoa="9" width="1" peak="2.5"/>
          <flap0 start="0" end="0.621" lift="1.6" drag="1.6"/>
          <flap1 start="0.621" end="1.000" lift="1.4" drag="1.3"/>
          <spoiler start="0.17" end="0.58" lift="6.0" drag="4.0"/>

          <control-input axis="/controls/flight/spoilers" control="SPOILER"/>
          <control-input axis="/controls/flight/flaps" control="FLAP0" invert="true"/>
          <control-input axis="/controls/flight/aileron" control="FLAP1" split="true"/>
          <control-input axis="/controls/flight/aileron-trim" control="FLAP1" split="true"/>

          <control-output control="SPOILER" prop="/surface-positions/speedbrake-pos-norm"/>  
          <control-output control="FLAP0" prop="/surface-positions/flap-pos-norm"/>
          <control-output control="FLAP1" side="left" prop="/surface-positions/left-aileron-pos-norm"/>
          <control-output control="FLAP1" side="right" prop="/surface-positions/right-aileron-pos-norm"/>

          <control-speed control="FLAP0" transition-time="5"/>
    </wing>

    <!-- Elevator -->
    <hstab x="-13.742" y="0" z="-0.803"
           chord="4.107"
           length="7.367"
           taper="0.46"
           sweep="3">
           <stall aoa="8" width="1" peak="1.4"/>
           <flap0 start="0.15" end="1" lift="1.8" drag="1.7"/>

           <control-input axis="/controls/flight/elevator" control="FLAP0"/>
           <control-input axis="/controls/flight/elevator-trim" control="FLAP0"/>

           <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>
    </hstab>

    <!-- Ruder -->
    <vstab x="-13.002" y="0" z="-0.148"
           chord="6.320"
           length="6.158"
           taper="0.55"
           sweep="14">
           <stall aoa="8" width="1" peak="1.5"/>
           <flap0 start="0" end="1" lift="1.3" drag="1.5"/>

           <control-input axis="/controls/flight/rudder" control="FLAP0" invert="true"/>
           <control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>

           <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"/>
    </vstab>

    <!-- Informations : http://en.wikipedia.org/wiki/Rolls-Royce_Tyne
                        http://www.aviation-fr.info/guide/transall.php

         Engines : Wright Rolls-Royce Tyne 22
         type    : Twin-spool turboprop
         Power   : 6100 ch
         Masse   : 2489 lb (1129 kg)
         RPM     : 2400 - 2600
    -->
    <!-- Engine left 
    <propeller x="6.033" y="5.411" z="-1.424"
               mass="2489"
               moment="8"
               radius="2.8"
               cruise-speed="260"
               cruise-rpm="2400"
               cruise-power="5300"
               cruise-alt="17000"
               min-rpm="200"
               max-rpm="2300">
               <actionpt x="7.750" y="5.411" z="-1.424"/>
               <control-input axis="/controls/engines/engine[0]/propeller-pitch" control="ADVANCE"/>
               <turbine-engine eng-power="6100"
                               eng-rpm="2600"
                               alt="8000"
                               flat-rating="550"
                               bsfc="0.495"
                               compression="7.2"
                               max-n2="101"> 
                               <control-input axis="/controls/engines/engine[0]/throttle" control="THROTTLE"/>
                               <control-input axis="/controls/engines/engine[0]/starter" control="STARTER"/>
                               <control-input axis="/controls/engines/engine[0]/magnetos" control="MAGNETOS"/>
                               <control-input axis="/controls/engines/engine[0]/mixture" control="MIXTURE"/>
               </turbine-engine>
    </propeller>
-->
    <!-- Engine right 
    <propeller x="6.033" y="-5.411" z="-1.424"
               mass="2489"
               moment="8"
               radius="2.8"
               cruise-speed="260"
               cruise-rpm="2400"
               cruise-power="5300"
               cruise-alt="17000"
               min-rpm="200"
               max-rpm="2300">
               <actionpt x="7.750" y="-5.411" z="-1.424"/>
               <control-input axis="/controls/engines/engine[1]/propeller-pitch" control="ADVANCE"/>
               <turbine-engine eng-power="6100"
                               eng-rpm="2600"
                               alt="8000"
                               flat-rating="550"
                               bsfc="0.495"
                               compression="7.2"
                               max-n2="101"> 
                               <control-input axis="/controls/engines/engine[1]/throttle" control="THROTTLE"/>
                               <control-input axis="/controls/engines/engine[1]/starter" control="STARTER"/>
                               <control-input axis="/controls/engines/engine[1]/magnetos" control="MAGNETOS"/>
                               <control-input axis="/controls/engines/engine[1]/mixture" control="MIXTURE"/>
               </turbine-engine>
    </propeller>
-->
    <propeller x="6.033" y="5.411" z="-1.424"
               mass="2489"
               moment="-8"
               radius="2.8"
               cruise-speed="260"
               cruise-rpm="2400"
               cruise-power="5300"
               cruise-alt="17000"
               min-rpm="100"
               max-rpm="2300">
               <actionpt x="7.750" y="5.411" z="-1.424"/>
               <control-input axis="/controls/engines/engine[0]/propeller-pitch" control="ADVANCE"/>
               <piston-engine eng-power="6100"
                              eng-rpm="2600"
                              alt="28000"> 
                              <control-input axis="/controls/engines/engine[0]/throttle" control="THROTTLE"/>
                              <control-input axis="/controls/engines/engine[0]/starter" control="STARTER"/>
                              <control-input axis="/controls/engines/engine[0]/magnetos" control="MAGNETOS"/>
                              <control-input axis="/controls/engines/engine[0]/mixture" control="MIXTURE"/>
               </piston-engine>
    </propeller>

    <!-- Engine right-->
    <propeller x="6.033" y="-5.411" z="-1.424"
               mass="2489"
               moment="-8"
               radius="2.8"
               cruise-speed="260"
               cruise-rpm="2400"
               cruise-power="5300"
               cruise-alt="17000"
               min-rpm="100"
               max-rpm="2300">
               <actionpt x="7.750" y="-5.411" z="-1.424"/>
               <control-input axis="/controls/engines/engine[1]/propeller-pitch" control="ADVANCE"/>
               <piston-engine eng-power="6100"
                              eng-rpm="2600"
                              alt="28000"> 
                              <control-input axis="/controls/engines/engine[1]/throttle" control="THROTTLE"/>
                              <control-input axis="/controls/engines/engine[1]/starter" control="STARTER"/>
                              <control-input axis="/controls/engines/engine[1]/magnetos" control="MAGNETOS"/>
                              <control-input axis="/controls/engines/engine[1]/mixture" control="MIXTURE"/>
               </piston-engine>
    </propeller>

    <!-- Noze wheel -->
    <gear x="12.970" y="0.000" z="-5.737"
          compression="0.2"
          retract-time="5"
          spring="0.4"
          damp="1.50">
          <control-input axis="/controls/flight/rudder" control="STEER"  src0="-1.0" src1="1.0" dst0="-0.7" dst1="0.7" />
          <control-input axis="/controls/gear/gear-down" control="EXTEND"/>
          <control-output control="EXTEND" prop="/gear/gear[0]/position-norm"/>
          <control-speed control="EXTEND" transition-time="5"/>
    </gear>

    <!-- Left wheel 1 -->
    <gear x="3.266" y="2.502" z="-5.802"
          compression="0.2"
          retract-time="5"
          spring="0.7"
          damp="1.5"
          sfric="1.2"
          dfric="1.2">
          <control-input axis="/controls/flight/rudder" control="STEER" src0="-1.0" src1="1.0" dst0="-0.08" dst1="0.08"/>
          <control-input axis="/controls/gear/brake-left" control="BRAKE"/>
          <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
          <control-input axis="/controls/gear/gear-down" control="EXTEND"/>
          <control-speed control="EXTEND" transition-time="5.1"/>
          <control-output control="EXTEND" prop="/gear/gear[1]/position-norm"/>
    </gear>

    <!-- Left wheel 2 -->
    <gear x="1.702" y="2.502" z="-5.802"
          compression="0.2"
          retract-time="5"
          spring="0.7"
          damp="1.5"
          sfric="1.2"
          dfric="1.2">
          <control-input axis="/controls/flight/rudder" control="STEER" src0="-1.0" src1="1.0" dst0="0.08" dst1="-0.08"/>
          <control-input axis="/controls/gear/brake-right" control="BRAKE"/>
          <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
          <control-input axis="/controls/gear/gear-down" control="EXTEND"/>
          <control-speed control="EXTEND" transition-time="5.2"/>
          <control-output control="EXTEND" prop="/gear/gear[2]/position-norm"/>
    </gear>

    <!-- Right wheel 1 -->
    <gear x="3.266" y="-2.502" z="-5.802"
          compression="0.2"
          retract-time="5"
          spring="0.7"
          damp="1.5"
          sfric="1.2"
          dfric="1.2">
          <control-input axis="/controls/flight/rudder" control="STEER" src0="-1.0" src1="1.0" dst0="-0.08" dst1="0.08"/>
          <control-input axis="/controls/gear/brake-left" control="BRAKE"/>
          <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
          <control-input axis="/controls/gear/gear-down" control="EXTEND"/>
          <control-speed control="EXTEND" transition-time="5.1"/>
          <control-output control="EXTEND" prop="/gear/gear[3]/position-norm"/>
    </gear>

    <!-- Right wheel 2 -->
    <gear x="1.702" y="-2.502" z="-5.802"
          compression="0.2"
          retract-time="5"
          spring="0.7"
          damp="1.5"
          sfric="1.2"
          dfric="1.2">
          <control-input axis="/controls/flight/rudder" control="STEER" src0="-1.0" src1="1.0" dst0="0.08" dst1="-0.08"/>
          <control-input axis="/controls/gear/brake-right" control="BRAKE"/>
          <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
          <control-input axis="/controls/gear/gear-down" control="EXTEND"/>
          <control-speed control="EXTEND" transition-time="5.2"/>
          <control-output control="EXTEND" prop="/gear/gear[4]/position-norm"/>
    </gear>

    <tank x="2.7" y="6.0" z="-1" capacity="1600"/>
    <tank x="2.7" y="-6.0" z="-1" capacity="1600"/>

    <!-- CG Theorique Centre de la corde: x=2.451 y=0.0 z=-1.060" -->
    <ballast x="16" y="0" z="-1.5" mass="15000"/>

</airplane>


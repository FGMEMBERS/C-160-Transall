  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/C-160-Transall/Models/Liveries", 10);

      var self = cmdarg();

      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);

      var crew_door = self.getNode("sim/model/door-positions/crew/position-norm", 1);
      var passenger_door = self.getNode("sim/model/door-positions/passenger/position-norm", 1);

      crew_door.alias(float_0);
      passenger_door.alias(float_1);

      <!--       var eng1fuelon = self.getNode("/controls/engines/engine[0]/cutoff", 1),
      var eng2fuelon = func { setprop("/controls/engines/engine[1]/cutoff", 0); }

      var eng1fueloff = self.getNode("/controls/engines/engine[0]/cutoff", 1),
      var eng2fueloff = func { setprop("/controls/engines/engine[1]/cutoff", 1); }

      var eng1starter = self.getNode("/controls/engines/engine[0]/starter", 1);
      var eng2starter = func { setprop("/controls/engines/engine[1]/starter", 1); }

      var eng1stop    = self.getNode("/controls/engines/engine[0]/starter", 0);
      var eng2stop    = func { setprop("/controls/engines/engine[1]/starter", 0); }  -->
    </load>

    <!--     <generic-yasim-engine>
    <file>Aircraft/Generic/Engines/generic-yasim-engine.nas</file>
    </generic-yasim-engine> -->

    <engines-yasim>
      <file>Aircraft/C-160-Transall/Nasal/engines-yasim.nas</file>
    </engines-yasim>

    <!--     <file>Aircraft/Generic/Engines/generic-yasim-engine.nas</file>
    <file>Aircraft/C-160-Transall/Nasal/engines-yasim.nas</file> -->

    <unload>
      livery_update.stop();
      crew_door.unalias();
      passenger_door.unalias();
    </unload>
  </nasal>


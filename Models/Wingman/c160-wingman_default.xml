      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Sélection Pales/disque en fonction de "RPM" -->
  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
    <condition>
      <greater-than>
        <property>/engines/engine[1]/rpm</property>
        <value>1000</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur</object-name>
    <condition>
      <and>
        <greater-than>
          <property>/engines/engine[1]/rpm</property>
          <value>400.000</value>
        </greater-than>
        <less-than>
          <property>/engines/engine[1]/rpm</property>
          <value>1000.001</value>
        </less-than>
      </and>
    </condition>
  </animation>

  <!-- Rotation de l'hélice -->
  <animation>
    <type>spin</type>
    <object-name>heliceD</object-name>
    <condition>
      <greater-than>
        <property>/engines/engine[1]/rpm</property>
        <value> 25 </value>
      </greater-than>
    </condition>
    <property>/engines/engine[1]/rpm</property>
    <factor> 0.479 </factor>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
